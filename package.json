{
  "name": "graphql-mongo-redis-boilerplate",
  "version": "1.0.0",
  "description": "Production ready boilerplate for node graphql mongo redis application",
  "main": "index.js",
  "scripts": {
    "test": "npm run test",
    "lint": "node_modules/.bin/eslint src --fix",
    "build": "babel src -d dist",
    "start": "npm run build && node dist/index.js",
    "start:dev": "nodemon --exec babel-node --inspect=9984 src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Akhilesh-pilkhwal/node-graphql-mongo-redis-boilerplate.git"
  },
  "keywords": [
    "Graphql",
    "nodejs",
    "mongodb",
    "passportjs",
    "redis",
    "apollo"
  ],
  "author": "Akhilesh singh pilkhwal",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Akhilesh-pilkhwal/node-graphql-mongo-redis-boilerplate/issues"
  },
  "homepage": "https://github.com/Akhilesh-pilkhwal/node-graphql-mongo-redis-boilerplate#readme",
  "dependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.0",
    "apollo-server": "^2.16.1",
    "graphql": "^15.3.0",
    "lodash": "^4.17.19"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.6.0",
    "nodemon": "^2.0.4"
  }
}
